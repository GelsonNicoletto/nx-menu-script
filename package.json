{
  "name": "nx-menu-script",
  "displayName": "NX Menu Script",
  "description": "Syntax highlighting for nx-menu-script language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages","Snippets"
  ],

  
  "contributes": {
    "languages": [{
      "id": "men",
      "aliases": ["NXMenuScript", "men"],
      "extensions": [".men"],
      "configuration": "./men.language-configuration.json"
    },
    {
      "id": "dat",
      "aliases": ["NXMenuScript", "dat"],
      "extensions": [".dat"],
      "configuration": "./dat.language-configuration.json"
    },
    {
      "id": "tbr",
      "aliases": ["NXMenuScript", "tbr"],
      "extensions": [".tbr"],
      "configuration": "./tbr.language-configuration.json"
    },
    {
      "id": "abr",
      "aliases": ["NXMenuScript", "abr"],
      "extensions": [".abr"],
      "configuration": "./abr.language-configuration.json"
    },
    {
      "id": "btn",
      "aliases": ["NXMenuScript", "btn"],
      "extensions": [".btn"],
      "configuration": "./btn.language-configuration.json"
    },
    {
      "id": "grb",
      "aliases": ["NXMenuScript", "grb"],
      "extensions": [".grb"],
      "configuration": "./grb.language-configuration.json"
    },
    {
      "id": "gly",
      "aliases": ["NXMenuScript", "gly"],
      "extensions": [".gly"],
      "configuration": "./gly.language-configuration.json"
    },
    {
      "id": "rtb",
      "aliases": ["NXMenuScript", "rtb"],
      "extensions": [".rtb"],
      "configuration": "./rtb.language-configuration.json"
    }],



    "grammars": [{
      "language": "men",
      "scopeName": "source.men",
      "path": "./syntaxes/men.tmLanguage.json"
    },
    {
      "language": "dat",
      "scopeName": "source.dat",
      "path": "./syntaxes/dat.tmLanguage.json"
    },
    {
      "language": "tbr",
      "scopeName": "source.tbr",
      "path": "./syntaxes/tbr.tmLanguage.json"
    },
    {
      "language": "grb",
      "scopeName": "source.grb",
      "path": "./syntaxes/grb.tmLanguage.json"
    },
    {
      "language": "abr",
      "scopeName": "source.abr",
      "path": "./syntaxes/abr.tmLanguage.json"
    },
    {
      "language": "btn",
      "scopeName": "source.btn",
      "path": "./syntaxes/btn.tmLanguage.json"
    },
    {
      "language": "gly",
      "scopeName": "source.gly",
      "path": "./syntaxes/gly.tmLanguage.json"
    },
    {
      "language": "rtb",
      "scopeName": "source.rtb",
      "path": "./syntaxes/rtb.tmLanguage.json"
    }    
    ],


    "snippets": [
      {
        "language": "men",
        "path": "./snippets/men.snippets.json"
      }
    ]
  }
}
