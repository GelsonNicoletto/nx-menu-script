        "prefix" : "ACTIONS",
        "body" : [
            "ACTIONS ${0:STANDARD}"
        ]   
    },
    "actions-pre" : {
        "prefix" : "ACTIONS/PRE",
        "body" : [
            "ACTIONS/PRE ${0:Action}"
        ]   
    },
    "actions-post" : {
        "prefix" : "ACTIONS/POST",
        "body" : [
            "ACTIONS/POST ${0:Action}"
        ]   
